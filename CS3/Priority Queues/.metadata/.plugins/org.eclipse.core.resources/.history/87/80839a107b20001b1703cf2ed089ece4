
public class Student {
	private int ID;
	private String name;
	private int grade;
	private double gpa;
	public Student (int iD, String name, int grade, double gpa) {
		super();
		ID = iD;
		this.name = name;
		this.grade = grade;
		this.gpa = gpa;
	}
	public int getID() {
		return ID;
	}
	public void setID(int iD) {
		ID = iD;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getGrade() {
		return grade;
	}
	public void setGrade(int grade) {
		this.grade = grade;
	}
	public double getGpa() {
		return gpa;
	}
	public void setGpa(double gpa) {
		this.gpa = gpa;
	}
	
	
}
