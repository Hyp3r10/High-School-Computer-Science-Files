import java.util.ArrayList;
import java.util.Scanner;

public class OddDivisors {
	public static long values[];
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in); 
		int T = scan.nextInt(); scan.nextLine();
		for(int i = 0; i < T; i++) {
			Scanner line = new Scanner(scan.nextLine());
			long N = line.nextLong();
			long M = line.nextLong();
			values = new long[(int) N];
			System.out.println(getSum(N) % (M));
		}
	}
	public static long getSum(long N) {
		long result = 0;
		for(long i = 1; i <= N; i++) {
			long curr = getGreatestOdd(i);
			result += curr;
		}
		return result;
	}
	public static long getGreatestOdd(long index) {
		long greatestOdd;
		if(index % 2 == 1) {
			greatestOdd = (index);
			values[(int) index] = index; 
		}
		else {
			long mult = 1L;
			Long[] primeFactors = getPrimeFactors(index);
			for(int i = 0; i < primeFactors.length; i++) {
				if(primeFactors[i] % 2 == 1) {
					mult *= primeFactors[i];
				}
			}
			greatestOdd = mult;
		}
		return greatestOdd;
	}
	/*public static Long[] getPrimeFactors(long number) {
		ArrayList<Long> factors = new ArrayList<Long>();
		for(long i = 2; i < number; i++) {
			while(number%i == 0) {
				factors.add(i);
				number = number/i;
			}
		}
		factors.add(number);
		return factors.toArray(new Long[factors.size()]);
	}*/
 }
